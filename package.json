    "type": "git",
    "url": "https://github.com/Datasance/ECN-viewer"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@headlessui/react": "^2.2.3",
    "@mdi/js": "^7.4.47",
    "@msgpack/msgpack": "^3.1.2",
    "@mui/material": "7.3.7",
    "@types/leaflet": "^1.9.12",
    "ajv": "^8.18.0",
    "ajv-keywords": "^5.1.0",
    "apexcharts": "^3.45.0",
    "downshift": "9.0.6",
    "google-map-react": "1.1.6",
    "http-proxy-middleware": "3.0.5",
    "immutable": "^3.8.2",
    "js-yaml": "4.1.1",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.3",
    "lodash": "^4.17.23",
    "lucide-react": "^0.563.0",
    "moment": "^2.30.1",
    "oidc-client-ts": "^2.4.1",
    "pretty-bytes": "^6.1.1",
    "react": "^19.2.4",
    "react-ace": "^14.0.1",
    "react-apexcharts": "^1.4.0",
    "react-dnd": "^14.0.5",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "^19.2.4",
    "react-json-view": "^1.21.3",
    "react-leaflet": "4.2.0",
    "react-loading-skeleton": "^3.4.0",
    "react-oidc-context": "^3.3.0",
    "react-pro-sidebar": "^1.1.0",
    "react-router": "^7.13.0",
    "react-router-dom": "^7.13.0",
    "react-select": "^5.10.1",
    "sass": "^1.90.0",
    "swagger-ui-react": "^5.30.2",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0"
  },
  "proxy": "http://localhost:5555",
  "scripts": {
    "prebuild": "patch-package",
    "start": "npm run build && npm run prod",
    "start-dev": "cp controller-config.prod.js ./public/controller-config.js && react-scripts --openssl-legacy-provider start",
    "prod": "node server/index.js",
    "build": "cp controller-config.prod.js ./public/controller-config.js && GENERATE_SOURCEMAP=false react-scripts build",
    "build:electron": "cp public/controller-config.electron.js public/controller-config.js && GENERATE_SOURCEMAP=false PUBLIC_URL=. react-scripts build",
    "test": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "standard": "standard",
    "lint": "eslint --ext .js,.ts,.tsx src",
    "lint:fix": "eslint --ext .js,.ts,.tsx src --fix",
    "format": "prettier --write \"src/**/*.{js,ts,tsx,json,css,scss,md}\"",
    "fix-all": "npm run lint:fix && npm run format",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run start-dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build:electron && electron-builder",
    "electron:build:win": "npm run build:electron && electron-builder --win",
    "electron:build:mac": "npm run build:electron && electron-builder --mac",
    "electron:build:linux": "npm run build:electron && electron-builder --linux",
    "postinstall": "patch-package"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "resolutions": {
    "caniuse-lite": "1.0.30000984"
  },
  "overrides": {
    "axios": "^1.13.5",
    "prismjs": "^1.30.0",
    "js-yaml": "4.1.1",
    "minimatch": "10.2.1",
    "fork-ts-checker-webpack-plugin": {
      "minimatch": "9.0.5"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.16",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^19.1.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "electron": "^40.6.0",
    "electron-builder": "^26.8.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jest-enzyme": "^4.2.0",
    "nodemon": "^3.1.4",
    "patch-package": "^8.0.1",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.3.1",
    "sass-loader": "^16.0.5",
    "sinon": "^18.0.0",
    "standard": "^17.1.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "wait-on": "^9.0.4",
    "webpack": "^5.99.6",
    "webpack-cli": "^6.0.1"
  },
  "build": {
    "appId": "com.datasance.ecn-viewer",
    "productName": "ECN Viewer",
    "icon": "app-logo.png",
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "files": [
      "build/**/*",
      "electron/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "npmRebuild": false,
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm"
      ]
    }
  }
}
