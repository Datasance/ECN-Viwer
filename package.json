{
  "name": "@datasance/ecn-viewer",
  "version": "v1.4.0",
  "description": "Datasance ioFog Edge Cloud Network Viewer project  \\nCopyright (c) 2023 Datasance Teknoloji A.S.",
  "main": "electron/main.js",
  "author": "Emirhan Durmus",
